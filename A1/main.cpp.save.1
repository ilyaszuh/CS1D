#include <fstream>
#include <iostream>
#include <string>
using namespace std;
void mergeSort(int arr[], int start, int end);
void merge(int arr[], int start, int mid, int end)
void InsertionSort(int arr[], const int arrSIZE);

int main(int argc, char *argv[])
{
	
	int arrSIZE = 0;
	int *arr;
	int index = 0;
	string fileName = argv[1];
   ifstream inFile(fileName);
	int tempNum = 0;
	while(!inFile.eof())
	{
		inFile >> tempNum;
		arrSIZE++;
		tempNum = 0;
	}
	inFile.close();
	arrSIZE--;
   arr = new int[arrSIZE];
	inFile.open(fileName);

	while(!inFile.eof() && index < arrSIZE)
	{
		inFile >> arr[index];
		index++;
	}

    InsertionSort(arr, arrSIZE);

	delete [] arr;
	inFile.close();
	return 0;
}

void InsertionSort(int arr[], const int arrSIZE)
{
    int key = 0;
    int i = 0;
    for(int j = 1; j < arrSIZE; j++)
    {
        key = arr[j];
        i = j - 1;
        while(i >= 0 && arr[i] > key)
        {
            arr[i + 1] = arr[i];
            i--;
        }
        arr[i + 1] = key;
    }
}
